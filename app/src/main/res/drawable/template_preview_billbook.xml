<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
    <item>
        <shape android:shape="rectangle">
            <solid android:color="#FFFFFF" />
            <stroke android:width="1dp" android:color="#444444" />
        </shape>
    </item>
    <item android:top="40dp" android:left="20dp" android:right="20dp" android:height="50dp">
        <shape android:shape="rectangle">
            <solid android:color="#EBF5DC" />
            <stroke android:width="0.5dp" android:color="#444444" />
        </shape>
    </item>
    <item android:top="90dp" android:bottom="150dp" android:left="20dp" android:right="20dp">
        <shape android:shape="rectangle">
            <solid android:color="#FFFFFF" />
            <stroke android:width="0.5dp" android:color="#444444" />
        </shape>
    </item>
    <item android:bottom="100dp" android:top="90dp" android:left="20dp" android:right="20dp">
        <shape android:shape="rectangle">
            <solid android:color="#00FFFFFF" />
            <stroke android:width="0.5dp" android:color="#444444" dashWidth="3dp" dashGap="2dp" />
        </shape>
    </item>
</layer-list>